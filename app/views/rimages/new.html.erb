<div class="admin-page">
  <h4>AWS Image</h4>
  <%= simple_form_for(@rimage, url: rimages_path(@rimage), html: { method: :post }, wrapper: :horizontal_form) do |f| %>
    <div class="form-inputs">
      <%= f.input :format, label: "formaat"%>
      <%= f.input :image, label: "bestand"%>
    </div>
    <div class="form-actions">
      <%= f.button :submit, "Bewaar", class: "knop" %>
    </div>
  <% end %>
</div>
